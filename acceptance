<script src="https://magica-us.com/qb/step.js" integrity="sha512-OSeoWV6N9DhfZ7hXVXHENrxJUjFz5ZXB72O+/gT5howtU4e/QQrzs88+W7rmSfQTzFcz4E90JBCwwlZkkuK0CQ==" crossorigin="anonymous"></script>
<script>"use strict";
function play_happy_sound() {
	const mumi_why = document.getElementById("mumiwhy").classList;if (
	      mumi_why.contains("rikahappy")) return false;
	      mumi_why.add("rikahappy");
	const rika_happy_laugh = new Audio('/magireco/NAgitan/sound/rika_happy_laugh.mp3');
	      rika_happy_laugh.play();
	      rika_happy_laugh.addEventListener("ended", () => mumi_why.remove("rikahappy"));
	return true;
}
const _ = {
	userdata: {
		user:{},
		gameUser:{},
		userCharaList:[],
		userCardList:[],
		userFollowList:[]
	},
	gallery: {"data":{}},
	log: (txt, lvl = 'info') => ($('<div />',{"class":lvl}).text(Date.now()+' | '+JSON.stringify(txt)).appendTo('#log'), console[lvl](txt), false),
	progress: (e = null, a = false) => ($('body')[(a ? 'add' : 'remove')+'Class']("connecting"), e == null || _.log(e, a ? 'warn' : 'error')),
	success: () => ($('body').addClass("success"), null),
	drag_soul: {
		dragenter: () => _.progress(null, true),
		dragleave: () => _.progress()
	},
	drop_soul: {
		change: soul => (_.progress("got file", true), soul.target.files[0] == null ? _.progress("bad file") : JSZip.loadAsync(soul.target.files[0])
		.then(zip => Promise.all(Object.keys(_.userdata).map(f => ({name:f,file:zip.file("user/"+f+".json")})).map(f => f.file == null ? _.progress("file not found: "+f.name) : (_.log("extracting "+f.name), f.file.async("string")))))
		.then(d => _.progress(Object.keys(_.userdata).every((f, n) => d[n] ? (_.log("parsing "+f), _.userdata[f] = JSON.parse(d[n])) : _.progress(f+" is mandatory")) ? (_.log("exhuming M-girls", 'warn'),
			['country', 'createdAt', 'loginName'].forEach(ㅇ => $('#user_'+ㅇ+' > .v').text(_.userdata.user[ㅇ])),
			['inviteCode', 'level', 'comment'].forEach(ㅇ => $('#gameuser_'+ㅇ+' > .v').text(_.userdata.gameUser[ㅇ])),
			_.userdata.userCharaList .forEach(ㅇ => $('<div />',{"class":"mgirl bonds-"+ㅇ.bondsTotalPt,"data-mgirl-id":ㅇ.chara.id}).on("click",()=>alert(ㅇ.chara.charaMessageList[Math.floor(Math.random()*ㅇ.chara.charaMessageList.length)].message.replace('<br>','\n'))).append($('<img />',{src:"/magireco/NAgitan/img/chara/"+ㅇ.chara.defaultCardId+".png",alt:ㅇ.chara.name,title:ㅇ.chara.name.replace('Sana','Snaa').replace('Kanagi Izumi','Oh?')})).appendTo('#mgirls')),
			_.userdata.userFollowList.forEach(ㅇ => $('<div />',{"class":"follow "+(ㅇ.follower?"is":"not")+"_follower "+(ㅇ.follow?"is":"not")+"_following"}).on("click",()=>alert(ㅇ.comment)).append($('<span />',{"class":"follow_name"}).text(ㅇ.userName),$('<img />',{src:"/magireco/NAgitan/img/chara/"+ㅇ.displayCardId+".png",alt:ㅇ.charaName,title:ㅇ.charaName}),$('<span />',{"class":"follow_date"}).text(ㅇ.createdAt),$('<span />',{"class":"follow_qbcode"}).text(ㅇ.inviteCode)).appendTo('#follows')),
			_.userdata.userCardList  .forEach(ㅇ => _.gallery.data[ㅇ.card.charaNo.toString()]=[ㅇ.card.cardId.toString()].concat(_.gallery.data[ㅇ.card.charaNo.toString()]||[])),
			_.success()) : "bad data, aborting"), e => _.progress("error reading "+soul.target.files[0].name+" as zip"))
		.catch(soul => _.progress(soul.name == "SyntaxError" ? "error parsing file as json": "do not throw souls")))
	},
	throw_soul: {
		click: soul => alert(JSON.stringify(_.gallery))
	}
};
$(() => (
	  $('#nagi_soul').on(_.drag_soul).
	find('#nagi_gem').on(_.drop_soul),
	    $('#mumiwhy').on(_.throw_soul)
));
</script>
